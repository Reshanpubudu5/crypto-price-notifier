{
  "manifest_version": 3,
  "name": "B-Coin Tracker",
  "description": "Track Binance Coin Value Changes",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
