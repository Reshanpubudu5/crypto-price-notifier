{
  "manifest_version": 3,
  "name": "Binance Tracker",
  "description": "Track Binance Value Changes",
  "version": "1.0.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
